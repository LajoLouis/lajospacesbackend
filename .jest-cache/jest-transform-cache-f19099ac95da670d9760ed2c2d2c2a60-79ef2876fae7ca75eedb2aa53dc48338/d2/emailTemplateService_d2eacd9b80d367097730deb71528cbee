      cov_1z8bquhe7f().s[36]++;
      logger_1.logger.error(`Error rendering template ${templateType}:`, error);
      /* istanbul ignore next */
      cov_1z8bquhe7f().s[37]++;
      throw new Error(`Failed to render template: ${templateType}`);
    }
  }
